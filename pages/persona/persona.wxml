<view class="body">
    <view class="user-info">
        <image class="avatar" src="{{avatarUrl}}"></image>
        <view class="info">
            <view class="block">
                <text class="name">{{nickname}}</text>
                <button class="bnt">切换角色</button>
            </view>
        </view>
    </view>
    <view class="rblock">
        <view class="lblock" bindtap='toInfo'>
            <image class="bpic" src="../../static/消息.png" />
            <text >个人信息</text>
        </view>
        <view class="lblock">
            <image class="bpic" src="../../static/我的班级.png" />
            <text>我的班级</text>
        </view>
        <view class="lblock">
            <image class="bpic" src="../../static/收藏.png" />
            <text>我的收藏</text>
        </view>
    </view>
    <mp-cells ext-class="my-cells">
        <mp-cell ext-class="cel" value="关于我们">
            <image class="pic" slot="icon" src="../../static/关于我们.png" />
        </mp-cell>
        <mp-cell ext-class="cel" value="用户协议">
            <image class="pic" slot="icon" src="../../static/用户协议.png" />
        </mp-cell>
        <mp-cell ext-class="cel" value="清理缓存">
            <image class="pic" slot="icon" src="../../static/清理缓存.png" />
        </mp-cell>
    </mp-cells>
    <mp-cells ext-class="my-cells">
        <mp-cell ext-class="cel" value="客服">
            <image class="pic" slot="icon" src="../../static/客服.png" />
        </mp-cell>
        <mp-cell ext-class="cel" value="设置">
            <image class="pic" slot="icon" src="../../static/设置.png" />
        </mp-cell>
    </mp-cells>
</view>